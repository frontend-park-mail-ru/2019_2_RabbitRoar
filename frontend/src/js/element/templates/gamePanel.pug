div(class="game-panel-container")
    label(for="answer")
    input(class="input-answer" id="answer", type="text")
    div(class="profile")
        div(class="logo-div")
            img(class="game-panel__user-logo",
            src=userData.avatar_url,
            alt="User")
        div(class="panel-text") #{userData.username}            
    div(id="changing-element" class="control")
        button(class="game-panel-button" id="game-panel-button" type="submit") Ответить
    div(class="score")
        div
            p(class="panel-text") Счет: 
            p(class="panel-text" id="score") 0